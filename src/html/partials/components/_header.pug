// Header
+b.HEADER.header
  .container
    +e.inner
      +e.logo
        // Logo
        +b.A.logo(href="index.html", itemscope, itemtype="http://schema.org/Brand") 
          img(src="img/logo.svg", alt="logo", title="logo", itemprop="logo")
          //- +e.H2.title
          //-   span.blue Тест Петербург
          //-   span.orange Сертификация

      +e.nav
        // Nav
        +b.NAV.nav
          +e.UL.list
            .menu-item 
              a(href="page-services.html") Услуги
            .menu-item 
              a(href="page-about.html") О компании
            .menu-item 
              a(href="page.html") Faq
            .menu-item 
              a(href="page-news.html") Новости
            .menu-item
              a(href="page-contacts.html") Контакты
          +e.hamburger
